{
  "name": "cdk-iam-role-external-access",
  "version": "0.1.0",
  "bin": {
    "cdk-iam-role-external-access": "dist/bin/app.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-lambda",
    "clean": "rm -rf dist",
    "copy-lambda": "cp -r src/Lambda/package.json dist/src/Lambda/ 2>/dev/null || true",
    "watch": "tsc -w",
    "test": "npm run build && jest",
    "cdk": "npm run build && cdk",
    "deploy": "npm run build && cdk deploy",
    "synth": "npm run build && cdk synth",
    "diff": "npm run build && cdk diff", 
    "prebuild": "npm run clean"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@types/aws-lambda": "^8.10.145",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1029.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.214.0",
    "constructs": "^10.0.0",
    "@aws-sdk/client-secrets-manager": "^3.665.0"
  }
}
